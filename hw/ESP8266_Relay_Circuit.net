# ESP8266 Relay Control Circuit - KiCAD Style Netlist
# Circuit: ESP8266 NodeMCU to Omron G6B-1114P-US 12VDC Relay Control

(export (version D)
  (design
    (source ESP8266_Relay_Control.sch)
    (date "2024-02-15")
    (tool "Manual Netlist Generator")
  )
  (components
    (comp (ref U1)
      (value ESP8266_NodeMCU)
      (footprint ESP8266:NodeMCU)
      (description "ESP8266 NodeMCU ESP-12E Development Board")
      (fields
        (field (name Manufacturer) Espressif)
        (field (name PartNumber) ESP-12E)))
    (comp (ref Q1)
      (value 2N2222)
      (footprint TO-92)
      (description "NPN Transistor, 600mA, 40V")
      (fields
        (field (name Manufacturer) Various)
        (field (name Alternatives) "BC547, 2N3904, PN2222")))
    (comp (ref R1)
      (value 1k)
      (footprint Resistor_THT:R_Axial_DIN0207)
      (description "1kΩ Resistor, 1/4W")
      (fields
        (field (name Tolerance) "5%")
        (field (name Power) "0.25W")))
    (comp (ref D1)
      (value 1N4007)
      (footprint Diode_THT:D_DO-41)
      (description "1A 1000V Rectifier Diode")
      (fields
        (field (name Manufacturer) Various)
        (field (name Alternatives) "1N4001-1N4007 series")))
    (comp (ref K1)
      (value G6B-1114P-US)
      (footprint Relay_THT:Relay_SPST_Omron_G6B)
      (description "Omron SPST-NO Relay, 12VDC, 5A")
      (fields
        (field (name Manufacturer) Omron)
        (field (name PartNumber) G6B-1114P-US)
        (field (name CoilVoltage) 12VDC)
        (field (name CoilResistance) "400Ω")
        (field (name CoilCurrent) "30mA")
        (field (name ContactRating) "5A 250VAC / 5A 30VDC")))
    (comp (ref PS1)
      (value 12VDC)
      (footprint TerminalBlock:TerminalBlock_2x5mm)
      (description "12V DC Power Supply")
      (fields
        (field (name MinCurrent) "100mA")))
  )
  (nets
    (net (code 1) (name "GPIO5_D1")
      (node (ref U1) (pin D1_GPIO5))
      (node (ref R1) (pin 1)))
    (net (code 2) (name "TRANSISTOR_BASE")
      (node (ref R1) (pin 2))
      (node (ref Q1) (pin BASE)))
    (net (code 3) (name "GND")
      (node (ref U1) (pin GND))
      (node (ref Q1) (pin EMITTER))
      (node (ref PS1) (pin NEG))
      (node (ref D1) (pin ANODE)))
    (net (code 4) (name "+12V")
      (node (ref PS1) (pin POS))
      (node (ref K1) (pin COIL_PLUS))
      (node (ref D1) (pin CATHODE)))
    (net (code 5) (name "RELAY_COIL_NEG")
      (node (ref K1) (pin COIL_MINUS))
      (node (ref Q1) (pin COLLECTOR)))
    (net (code 6) (name "RELAY_COM")
      (node (ref K1) (pin COM))
      (connection "To Load Common"))
    (net (code 7) (name "RELAY_NO")
      (node (ref K1) (pin NO))
      (connection "To Load Switched"))
  )
)
